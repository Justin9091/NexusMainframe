<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.yourcompany.nexus</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/NexusMainFrame</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>

    <key>WorkingDirectory</key>
    <string>/usr/local/var/nexus</string>

    <key>StandardOutPath</key>
    <string>/usr/local/var/nexus/logs/nexus.log</string>

    <key>StandardErrorPath</key>
    <string>/usr/local/var/nexus/logs/nexus-error.log</string>

    <key>UserName</key>
    <string>_nexus</string>

    <key>GroupName</key>
    <string>_nexus</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>

    <key>ProcessType</key>
    <string>Background</string>

    <key>Nice</key>
    <integer>0</integer>
</dict>
</plist>